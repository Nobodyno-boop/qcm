    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    {% block assets %}
        {#        <script src="https://cdn.tailwindcss.com"></script> #}
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/jgthms/minireset.css@master/minireset.min.css">
